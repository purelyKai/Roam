{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobile.mobile",
      "config": {
        "googleMapsApiKey": "AIzaSyBQ7ygAw0dp2fiF7abThmbRKlsrQbHBbQc"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Roam to use your location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Roam to use your location",
        "NSLocationAlwaysUsageDescription": "Allow Roam to use your location",
        "UIBackgroundModes": ["location"]
      }
    },
    "android": {
      "package": "com.mobile.mobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBQ7ygAw0dp2fiF7abThmbRKlsrQbHBbQc"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Roam to use your location."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.466033117237-j9tbq92puccbv6820v0bj8307jb2co34"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
